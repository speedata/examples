<Layout xmlns="urn:speedata.de:2009/publisher/en" >
  <Options crop="3mm"/>

  <!-- This example uses "ForAll" to visit each child node -->
  <Record element="root">
    <ForAll select="child">
      <PlaceObject>
        <Barcode select="string(.)" type="Code128" width="4"/>
      </PlaceObject>
      <NextRow/>
    </ForAll>
  </Record>

</Layout>
